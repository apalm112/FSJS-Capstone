		"cors": "^2.8.4",
		"isomorphic-fetch": "^2.2.1",
		"react": "^16.3.0",
		"react-dom": "^16.3.0",
		"react-router-dom": "^4.2.2",
		"serialize-javascript": "^1.4.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.6",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"eslint": "^5.8.0",
